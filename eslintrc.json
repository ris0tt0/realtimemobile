{
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:jest/recommended"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly",
		"fetch":false
	},
	"parserOptions": {
		"ecmaFeatures": {
				"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": [
		"react",
		"babel",
		"jest"
	],
	"parser": "babel-eslint",
	"rules": {
		"react/jsx-uses-react": "error",
		"react/jsx-uses-vars": "error"
	}
}