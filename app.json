{
  "expo": {
    "name": "Real Time BART",
    "slug": "rt-bart",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#009bda"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
		"ios": {
			"supportsTablet": true,
      "bundleIdentifier": "com.jonathangee.realtimebart"
    },
    "android": {
			"package": "com.jonathangee.realtimebart",
			"permissions":["LOCATION"],
			"versionCode":3
    }
  }
}